<h1>Log In</h1>
  <%= form_for :login, {url: login_path} do |f| %><br>
    <div>
      <%= f.email_field :email, placeholder: 'email' %>
    </div><br>

    <div>
      <%= f.password_field :password, placeholder: 'password' %>
    </div><br>

    <div>
      <%= f.submit 'Log In' %>
    </div>
  <% end %>
